{
  "manifest_version": 3,
  "name": "Zoho Timesheet Goal",
  "description": "A simple extension to track Zoho timesheet progress toward monthly goals.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/vendor.js",
        "js/app.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}